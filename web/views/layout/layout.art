<!DOCTYPE html>
<html lang="en">
    {{ include "./head.art" }}
    <body>
        <!-- 顶部 -->
        {{ include './header.art'}}

        <!-- 内容 -->

        {{ block 'body'}}

        {{ /block}}

        <!-- 底部 -->
        {{ include './footer.art'}}

        <!-- 加载vue1 -->
        <script type="text/javascript" src="/node_modules/vue/dist/vue.global.js"></script>
        <!-- 更改vue配置 -->
        <script>
            const { createApp } = Vue;

            const app = createApp({});
            app.config.devtools = true;
            console.log(app);
            app._component.delimiters = ['{%', '%}'];
        </script>

        {{ block 'script' }}
        {{ /block }}
    </body>
</html>
